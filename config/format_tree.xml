<?xml version="1.0" encoding="UTF-8"?>
<!-- A format tree specifying formats from general to specific.  The "format" attribute is denoted as a search 
string where * character is used in a way similar to wild card search. Therefore, "*Portable Document Format" will
match all PDF formats including "Acrobat PDF 1.0 - Portable Document Format", "Acrobat PDF 1.1 - Portable Document Format",
etc. -->

<format-tree>
	<branch>
		<format>Acrobat PDF 1.0 - Portable Document Format</format>
		<format>Acrobat PDF 1.1 - Portable Document Format</format>
		<format>Acrobat PDF 1.2 - Portable Document Format</format>
		<format>Acrobat PDF 1.3 - Portable Document Format</format>
		<format>Acrobat PDF 1.4 - Portable Document Format</format>
		<format>Acrobat PDF 1.5 - Portable Document Format</format>	
		<format>Acrobat PDF 1.6 - Portable Document Format</format>								
		<branch>
			<format>Acrobat PDF/A - Portable Document Format</format>
			<format>Acrobat PDF/X - Portable Document Format - Exchange 1:1999</format>
		</branch>
	</branch>
	<branch>
		<format>Tagged Image File Format</format>						
		<branch>
			<format>Tagged Image File Format for Image Technology (TIFF/IT)</format>
			<format>Tagged Image File Format for Electronic Still Picture Imaging (TIFF/EP)</format>
			<format>Tagged Image File Format for Internet Fax (TIFF-FX)</format>			
			<format>GeoTIFF</format>			
			<format>Digital Negative Format (DNG)</format>
		</branch>
	</branch>	
</format-tree>
